------------------ Configuração para R1 ------------------

conf t
ipv6 unicast-routing

int f0/0
 ip address 193.2.146.1 255.255.255.0
 ipv6 address 2001:2:cafe:2::1/64
 no shutdown
 ip ospf cost 10

int f0/1
 ip address 193.2.145.1 255.255.255.0
 ipv6 address 2001:2:cafe:3::1/64
 no shutdown
 ip ospf cost 10

int f1/0
 ip address 193.2.144.1 255.255.255.0
 ipv6 address 2001:2:cafe:1::1/64
 no shutdown
 ip ospf cost 10

router ospf 1
 router-id 1.1.1.1
 network 193.2.144.0 0.0.0.255 area 0
 network 193.2.145.0 0.0.0.255 area 0
 network 193.2.146.0 0.0.0.255 area 0

end
wr

----------------- Configuração para SwR2 -----------------

