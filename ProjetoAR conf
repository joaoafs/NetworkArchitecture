 ipv6 address 2001:2:cafe:4::2/64
 no shutdown
 ip ospf cost 10

int f2/0
 ip address 193.2.148.2 255.255.255.0
 ipv6 address 2001:2:cafe:5::2/64
 no shutdown
 ip ospf cost 10

int vlan1
 ip address 193.2.149.2 255.255.255.0
 ipv6 address 2001:2:cafe:6::2/64
 no shutdown
 ip ospf cost 10

router ospf 1
 router-id 2.2.2.2
 network 193.2.146.0 0.0.0.255 area 0
 network 193.2.147.0 0.0.0.255 area 0
 network 193.2.148.0 0.0.0.255 area 0
