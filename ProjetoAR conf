------------------ Configuração para R5 ------------------

conf t
ipv6 unicast-routing

int f0/0
 ip address 193.2.160.5 255.255.255.128
 ipv6 address 2001:2:cafe:9::5/64
 no shutdown
 ip ospf cost 10

int f0/1
 ip address 193.2.151.5 255.255.255.0
 ipv6 address 2001:2:cafe:8::5/64
 no shutdown
 ip ospf cost 10
 
int f1/0
 ip address 193.2.148.5 255.255.255.0
 ipv6 address 2001:2:cafe:5::5/64
 no shutdown
 ip ospf cost 10
 

router ospf 1
 router-id 5.5.5.5
 network 193.2.148.0 0.0.0.255 area 0
 network 193.2.151.0 0.0.0.255 area 0
 network 193.2.160.0 0.0.0.127 area 0
end
wr


------------------ Configuração para R6 ------------------

conf t
ipv6 unicast-routing

int f0/0
 ip address 193.2.160.6 255.255.255.128
 ipv6 address 2001:2:cafe:9::6/64
 no shutdown
 ip ospf cost 10

int f0/1
 ip address 193.2.150.6 255.255.255.0
 ipv6 address 2001:2:cafe:7::6/64
 no shutdown
 ip ospf cost 10

router ospf 1
 router-id 6.6.6.6
 network 193.2.160.0 0.0.0.127 area 0
 network 193.2.150.0 0.0.0.255 area 0

end
wr


